{
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/index.py"
      }
    ],
    "crons": [
      {
        "path": "/api/cron/fetch-guides",
        "schedule": "0 */12 * * *" 
      }
    ]
}